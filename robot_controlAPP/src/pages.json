{
	"easycom": {
		"autoscan": true,
		"custom": {
			// uni-ui 规则如下配置
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "portal"
			}
		}
	    ,{
            "path" : "pages/wine/index",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "酒水间"
				
            }
            
        }
        ,{
            "path" : "pages/kitchen/index",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "厨房",
                "enablePullDownRefresh": false
            }
            
        },
		{
		    "path" : "pages/charge/index",
		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "充电桩",
		        "enablePullDownRefresh": false
		    }
		    
		},
		{
		    "path" : "pages/sendFood/index",
		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "餐桌",
		        "enablePullDownRefresh": false
		    }
		    
		}
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "turtle Control",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
